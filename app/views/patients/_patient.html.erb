<tr>
  <td><%= patient.name %></td>
  <td><%= patient.cpf %></td>
  <td><%= patient.birth_date %></td>
  <td>
    <%= link_to edit_patient_path(patient), class:"btn btn-primary btn-circle" do %>
      <i class="bi bi-pencil"></i>
    <% end %>
  </td>
  <td>
    <%= link_to patient_path(patient), method: :delete, data: {confirm: "Tem certeza?"},class:"btn btn-primary btn-circle" do %>
      <i class="bi bi-trash"></i>
    <% end %>
  </td>
</tr>